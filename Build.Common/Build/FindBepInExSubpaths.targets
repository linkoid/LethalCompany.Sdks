<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  
  <PropertyGroup>
    <FindBepInExSubpathsTargetsHasBeenImported>true</FindBepInExSubpathsTargetsHasBeenImported>
  </PropertyGroup>

  <PropertyGroup>
    <PluginDirectory Condition="'$(PluginDirectory)' == ''">$([MSBuild]::NormalizeDirectory($(BepInExDirectory), plugins)</PluginDirectory>
    <PluginOutputDirectory Condition="'$(PluginOutputDirectory)' == ''">$([MSBuild]::NormalizeDirectory($(BepInExDirectory), $(MSBuildProjectName))</PluginOutputDirectory>
  </PropertyGroup>
</Project>